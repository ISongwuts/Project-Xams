generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  user_id String @id
  username String @unique
  prefix String
  first_name String
  last_name String
  password String 
  tel String
  personal_id String
  role String
  createAt String
}